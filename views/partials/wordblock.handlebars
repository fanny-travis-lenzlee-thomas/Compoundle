
 <div class="rearranger">
<h2>How to Play:</h2>
        <h3>Rearrange the words below to form a chain of compound words</h3>
  <div id="imageListId">
{{#each wordsArray}}
  <div id="blockno{{@index}}" class="listitemClass">
    {{#if this}}
      <h2>{{this}}</h2>
    {{else}}
      <h2><input id="guess-box" /></h2>
    {{/if}}
  </div>
{{/each}}
 </div>
 
 <div id="outputDiv">
      <input id="outputvalues" type="text" value="" />
      <button id="submit-button">Submit</button>
    </div>

     <div id="correct-order" class="hidden">{{correctOrder}}</div>
      <div id="blank-word" class="hidden">{{blankWord}}</div>

    <div id="results-container"></div>

   </div>

     
